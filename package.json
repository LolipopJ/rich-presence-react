{
  "name": "rich-presence-react",
  "private": false,
  "version": "1.0.0-beta.2",
  "type": "module",
  "author": "LolipopJ",
  "description": "React component to display Discord rich presence",
  "license": "Unlicense",
  "homepage": "https://lolipopj.github.io/rich-presence-react/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LolipopJ/rich-presence-react.git"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "module": "./dist/rich-presence-react.js",
  "main": "./dist/rich-presence-react.umd.cjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/rich-presence-react.js",
      "require": "./dist/rich-presence-react.umd.cjs"
    },
    "./style.css": "./dist/rich-presence-react.css"
  },
  "scripts": {
    "dev": "vite --config vite.docs.config.ts",
    "build": "tsc --noEmit && vite build --config vite.lib.config.ts",
